<h2>
  <%="#{@user.email}" %>
</h2>
<table class="table">
  <tr>
    <th>Action</th>
    <th>Type</th>
    <th>Date</th>
  </tr>
  <% @user_sessions.each do |session| %>
    <tr>
      <td>
        <%= session.action %>
      </td>
      <td>
        <%= session.authentication %>
      </td>
      <td>
        <%= session.created_at %>
      </td>
    </tr>
  <% end %>
</table>