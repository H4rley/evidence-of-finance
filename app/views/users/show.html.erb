<h2>
  <%="#{@user.email}" %>
</h2>
<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Action</th>
      <th>Type</th>
      <th>Date</th>
    </tr>
  </thead>
  <% @user_sessions.each do |session| %>
    <tr>
      <td>
        <%= session.action %>
      </td>
      <td>
        <%= session.authentication %>
      </td>
      <td>
        <%= session.created_at %>
      </td>
    </tr>
  <% end %>
</table>